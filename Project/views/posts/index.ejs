<main>
  <h1>All the posts</h1>
  <div class="post-grid">
    <% posts.forEach(post => {%>
      <div>
      <a href="/posts/<%= post.id %>">
      <img width="200px" height="200px" src="<%= post.image %>" />
      <h3><%= post.title %></h3>
      </a>
      </div>
      <% }) %>
    </div>
</main>
